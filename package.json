{
  "name": "eleventy-starter",
  "version": "1.0.0",
  "description": "Starter for elventy websites",
  "author": "Goran Tenic",
  "license": "MIT",
  "scripts": {
    "dev": "run-s clean eleventy:dev",
    "build": "run-s clean eleventy:prod",
    "eleventy:dev": "ELEVENTY_ENV=devlopment eleventy --serve",
    "eleventy:prod": "ELEVENTY_ENV=production eleventy",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "clean-css": "^4.2.3",
    "critical": "^2.0.5",
    "cssesc": "^3.0.0",
    "eslint": "^7.9.0",
    "eslint-config-gtenic": "1.0.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "html-minifier": "^4.0.0",
    "inline-source": "^7.2.0",
    "jest": "^26.4.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": "eslint-config-gtenic"
  }
}
